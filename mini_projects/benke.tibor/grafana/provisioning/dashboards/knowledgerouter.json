{
  "uid": "knowledgerouter",
  "title": "KnowledgeRouter Monitoring",
  "tags": ["knowledgerouter"],
  "timezone": "browser",
  "schemaVersion": 16,
  "version": 1,
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Request Rate",
      "type": "graph",
      "gridPos": {"x": 0, "y": 0, "w": 12, "h": 8},
      "targets": [{"expr": "rate(knowledgerouter_requests_total[5m])", "legendFormat": "{{domain}}"}]
    },
    {
      "id": 2,
      "title": "Active Requests",
      "type": "graph",
      "gridPos": {"x": 12, "y": 0, "w": 12, "h": 8},
      "targets": [{"expr": "knowledgerouter_active_requests"}]
    }
  ],
  "time": {"from": "now-1h", "to": "now"}
}